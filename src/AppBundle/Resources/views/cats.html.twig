<a href="{{ path('add_cat') }}">Ajouter un chat</a>
<table>
    <thead>
        <tr>
            <th>name</th>
            <th>id</th>
            <th>color</th>
            <th>age</th>
            <th>master</th>
            <th>mouseKilled</th>
            <th>date</th>
        </tr>
    </thead>
    <tbody>
        {% for value in cats %}
            <tr>
                <td>{{value.name}}</td>
                <td>{{value.id}}</td>
                <td>{{value.color}}</td>
                <td>{{value.age}}</td>
                <td>{{value.master}}</td>
                <td>{{value.mouseKilled}}</td>
                <td>{{value.date.format("Y-m-d H:i:s")}}</td>
                <td><a href="{{ path('edit_cat', {'id': value.id}) }}">Edit</a></td>
                <td><a href="{{ path('del_cat', {'id': value.id}) }}">Delete</a></td>
            </tr>
        {% endfor %}
    </tbody>
</table>